{
    "name": "fansoro/fansoro",
    "type": "library",
    "description": "Fansoro is Modern Open Source Flat-File Content Management System",
    "keywords": ["fansoro", "php", "cms", "flat-file", "markdown"],
    "homepage": "http://fansoro.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Romanenko",
            "email": "awilum@msn.com",
            "homepage": "https://github.com/Awilum"
        }
    ],
    "support": {
        "issues": "https://github.com/fansoro/fansoro/issues"
    },
    "require": {
        "php": ">=5.5.9",
        "erusev/parsedown-extra": "0.7.*",
        "doctrine/cache": "1.*",
        "pimple/pimple": "3.*",
        "twig/twig": "1.*",
        "symfony/filesystem": "3.*",
        "symfony/finder": "3.*",
        "symfony/yaml": "3.*",
        "force/session" : "*",
        "force/arr" : "*",
        "force/http" : "*",
        "force/shortcode" : "*",
        "force/token" : "*",
        "force/url" : "*"
    },
    "autoload": {
        "psr-4": {
            "Fansoro\\": "src"
        },
        "files": [
            "src/boot/defines.php",
            "src/boot/shortcodes.php",
            "src/boot/filters.php",
            "src/boot/actions.php"
        ]
    }
}
